<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DayShiftsNumPicker">
        <attr name="dayOfWeek" format="enum">
            <enum name="monday" value="1" />
            <enum name="tuesday" value="2" />
            <enum name="wednesday" value="3" />
            <enum name="thursday" value="4" />
            <enum name="friday" value="5" />
            <enum name="saturday" value="6" />
            <enum name="sunday" value="7" />
        </attr>
        <attr name="shiftsNum" format="integer" />
        <attr name="android:textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PendingApplicationsView">
        <attr name="android:textSize" format="dimension" />
    </declare-styleable>
</resources>